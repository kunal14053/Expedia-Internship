{
  "manifest_version": 2,

  "name": "Test",
  "description": "This is a test extension",
  "version": "2.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Hello!",
    "default_popup": "popup.html"
  },

"content_security_policy": "script-src 'self' https://cdnjs.cloudflare.com/ajax/libs/Chart.js/2.6.0/Chart.bundle.js; object-src 'self'",


  "permissions": [
    "activeTab",
    "tabs",
    "<all_urls>",
    "storage"
  ]



}